<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ufo.sample">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/RXBTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SimpleActivity">
        </activity>
        <activity android:name=".FrameLayoutActivity">
        </activity>
        <activity android:name=".ScrollViewActivity">
        </activity>
        <activity android:name=".AutoRefreshActivity">
        </activity>
        <activity android:name=".RecyclerViewActivity">
        </activity>
        <activity android:name=".MaterialActivity">
        </activity>
        <activity android:name=".EmptyActivity">
        </activity>
        <activity android:name=".BelowActivity">
        </activity>
        <activity android:name=".CustomHeadViewActivity">
        </activity>
        <activity android:name=".CustomFootViewActivity">
        </activity>
        <activity android:name=".WebViewActivity">
        </activity>
    </application>

</manifest>